{
  ".pubnub.yml": [
    { "pattern": "^version: \"(.+)\"$", "cleared": true },
    { "pattern": "\/releases\/tag\/v([0-9]+\\.[0-9]+\\.[0-9]+(\\.[0-9]+)?)", "cleared": true }
  ],
  "core/pubnub_version_internal.h": [
    { "pattern": "#define PUBNUB_SDK_VERSION \"(.+)\"$", "cleared": true }
  ],
  "CMakeLists.txt": [
    { "pattern": "project\(c-core VERSION (.+)\)$", "cleared": true }
  ]
}
